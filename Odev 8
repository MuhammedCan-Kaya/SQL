--MADDE 3:
	UPDATE employee 
	SET name = 'Ahmet'
	WHERE name = 'Cyrillus';

	UPDATE employee 
	SET email = 'x@y.com'
	WHERE email = 'hfronsek1@zimbio.com';

	UPDATE employee 
	SET birthday = '1980-10-02'
	WHERE birthday = '1958-12-30';

	UPDATE employee 
	SET name = 'Ayşe'
	birthday = '1985-12-08'
	WHERE name = 'Cindy';

	UPDATE employee 
	SET name = 'Tommy'
	birthday = '1985-12-08'
	email = 'asd@dsa.com'
	WHERE name = 'Hardy';

--MADDE 4:
	DELETE FROM employee
	WHERE name = 'Cyrillus';

	DELETE FROM employee
	WHERE email = 'hfronsek1@zimbio.com';

	DELETE FROM employee
	WHERE birthday = '1958-07-28';

	DELETE FROM employee
	WHERE name = 'Caz' AND birthday = '1912-06-16';

	DELETE FROM employee
	WHERE id = 3;

        
